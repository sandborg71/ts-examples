{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "strictNullChecks": false
  },
  "include": ["**/*, *.ts"],
  "exclude": []
}
